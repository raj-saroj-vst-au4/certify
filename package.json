{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "dotenv -- turbo build",
    "clean": "turbo clean",
    "dev": "dotenv -- turbo dev --no-cache  --continue",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "test": "turbo test",
    "typecheck": "turbo typecheck"
  },
  "dependencies": {
    "autoprefixer": "^10.4.16",
    "dotenv-cli": "github:motdotla/dotenv-cli",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "packageManager": "npm@10.2.0"
}
