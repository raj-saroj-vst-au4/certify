datasource db {
    provider = "postgresql"
    url = env("DATABASE_URL")
    relationMode = "prisma"
}

generator client {
    provider = "prisma-client.js"
}

model Student {
    id  String  @id @default(cuid())
    name String 
    phone String
    email String
}

model Certificate {
    id String @id
    createdAt DateTime @default(now())
}